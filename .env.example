# Example environment configuration for local development
# Copy to .env and fill in values as needed

# Google Custom Search (used by e-commerce and brand-SEO keyword tools)
GOOGLE_CSE_ID=
GOOGLE_SEARCH_API_KEY=

# OpenRouter API key (used by Practice/3-litellm_agent)
OPENROUTER_API_KEY=

# Optional: override the default model for most agents
GENAI_MODEL=gemini-2.0-flash

# Selenium headless mode for brand-SEO search_result sub-agent (1=headless)
HEADLESS=1

# Optional: Wikipedia OAuth bearer for higher limits (used by 2-tool_agent)
WIKI_ACCESS_TOKEN=

# Optional: Vector proxy service (Agentic-Tools/e-commerce/vector_service.py)
UPSTREAM_VECTOR_URL=
